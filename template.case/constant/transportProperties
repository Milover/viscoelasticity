FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include		"<constant>/caseSetup"

transportModel	Newtonian;

nu				#eval{ $muInf / $rho };

BirdCarreauCoeffs
{
	nu0			nu0		[ 0 2 -1 0 0 0 0 ] #eval{ $mu0/$rho };
	nuInf		nuInf	[ 0 2 -1 0 0 0 0 ] #eval{ $muInf/$rho };
	k			k		[ 0 0  1 0 0 0 0 ] 1.902;
	n			n		[ 0 0  0 0 0 0 0 ] 0.22;
	a			a		[ 0 0  0 0 0 0 0 ] 1.25;
}

