FoamFile
{
    version     2.0;
    format      ascii;
	class		IODictionary;
    location    "constant";
    object      caseSetup;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// geometry
R			1;						// pipe diameter
a			#eval{ 0.667*$R };		// stenosis height
b			#eval{ 4*$R };			// stenosis half-length
L			#eval{ 50*$R };			// inlet/outlet length

// flow quantities
Re			200;					// Reynolds number

// blood
//rho			1060;					// density
//muInf		3.45e-3;				// dynamic viscosity, at shear = âˆž
//mu0			5.60e-2;				// dynamic viscosity, at shear = 0

// water
rho			998.2;
muInf		1.0518e-3;
mu0			1.0518e-3;

// avg. inlet velocity
u_avg		#eval{ vector($Re*$muInf / (2*$R*$rho), 0, 0) };

// as per Hagen-Poiseuille flow solution
u_max		#eval{ 2 * vector($Re*$muInf / (2*$R*$rho), 0, 0) };


#inputMode merge

