FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    location    "0";
    object      tauthird;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include		"<constant>/caseSetup"

dimensions		[1 -1 -2 0 0 0 0];

internalField	uniform (0 0 0 0 0 0);

boundaryField
{
	inlet
	{
		type			fixedValue;
		value			$internalField;
	}

	outlet
	{
		type			zeroGradient;
	}

	"wall.*"
	{
		type			zeroGradient;
	}

	"symmetry.*"
	{
		type			symmetry;
	}

	"empty.*"
	{
		type			empty;
	}
}

