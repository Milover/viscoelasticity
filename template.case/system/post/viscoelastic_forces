viscoelastric_forces.stenosis
{
	type			viscoelasticForces;
	libs			("$(FOAM_USER_LIBBIN)/libviscoelasticTransportModels.so");
	writeControl	timeStep;
	writeInterval	$short_write;
	log				false;
	p				p;
	U				U;
	CofR			( 0 0 0 );
	patches			( wall.stenosis );
}


